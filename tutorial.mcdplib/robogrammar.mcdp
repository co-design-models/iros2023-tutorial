    provides terrain [`terr]
    provides speed [m/s]
    provides payload [g]
    
    requires power [W]
    requires num_components [Nat]
    requires num_high_power_motors [Nat]
    requires num_low_power_motors [Nat]
    
    
    0 deg, 0 m/s, 0 deg/s, 0 g <--| null_robot |-->  0 W, 0, 0, 0
}
